            "args": [
                "-test.run",
                "MyTestFunction"
            ]
        },
        {
           "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "port":1234,
           "host": "127.0.0.1",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": [],
            "showLog": true,
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        }
    ]
